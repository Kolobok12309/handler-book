{
  "name": "@hb/back",
  "version": "1.0.0",
  "description": "",
  "author": "Kolobok12309 <sova123409@gmail.com>",
  "private": true,
  "license": "MIT",
  "main": "dist/src/main.js",
  "types": "dist/src/main.d.ts",
  "scripts": {
    "prebuild": "yarn run clean",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env DEBUG=@hb/back:* nest start --watch",
    "dev": "yarn run start:dev",
    "start:debug": "nest start --debug --watch",
    "start:prod": "yarn run migrations:prod && cross-env NODE_ENV=production node dist/src/main",
    "start:prod:debug": "cross-env NODE_ENV=production DEBUG=@hb/back:* node dist/src/main",
    "clean": "rimraf dist",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js",
    "migration:generate": "yarn run typeorm migration:generate -n",
    "migration:create": "yarn run typeorm migration:create -n",
    "migration:run": "yarn run typeorm migration:run",
    "migration:revert": "yarn run typeorm migration:revert",
    "seed:generate": "yarn run typeorm migration:generate -c seed -n ",
    "seed:create": "yarn run typeorm migration:create -c seed -n",
    "seed:run": "yarn run typeorm migration:run -c seed",
    "seed:revert": "yarn run typeorm migration:revert -c seed",
    "migrations:run": "yarn run migration:run && yarn run seed:run",
    "migrations:prod": "cross-env NODE_ENV=production yarn run typeorm migration:run && yarn run typeorm migration:run -c seed",
    "release": "standard-version",
    "release:patch": "yarn run release -- --release-as patch",
    "release:minor": "yarn run release -- --release-as minor",
    "release:major": "yarn run release -- --release-as major"
  },
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "dependencies": {
    "@hb/types": "^1.0.0",
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.17",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "^7.1.5",
    "@sentry/integrations": "^6.4.1",
    "@sentry/node": "^6.4.1",
    "@sentry/tracing": "^6.4.1",
    "@types/multer": "^1.4.5",
    "aws-sdk": "^2.931.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.1",
    "multer": "^1.4.2",
    "node-mailer": "^0.1.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "run-script-webpack-plugin": "^0.0.11",
    "standard-version": "^9.3.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3",
    "webpack": "^5.37.1",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/../$1"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
